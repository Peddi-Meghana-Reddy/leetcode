class Solution:
    def minBitwiseArray(self, nums: list[int]) -> list[int]:
        res = []
        for n in nums:
            if n % 2 == 0:
                res.append(-1)
            else:
                lowest_zero_bit = (~n) & (n + 1)
                res.append(n - (lowest_zero_bit >> 1))
        return res
