from heapq import heappush, heappop
class Solution:
    def processQueries(self, c, connections, queries):
        parent = list(range(c + 1))
        rank = [0] * (c + 1)
        def find(x):
            if parent[x] != x:
                parent[x] = find(parent[x])
            return parent[x]
        def union(a, b):
            pa, pb = find(a), find(b)
            if pa == pb:
                return
            if rank[pa] < rank[pb]:
                parent[pa] = pb
            elif rank[pa] > rank[pb]:
                parent[pb] = pa
            else:
                parent[pb] = pa
                rank[pa] += 1
        for u, v in connections:
            union(u, v)
        comp_heap = {}
        for i in range(1, c + 1):
            root = find(i)
            if root not in comp_heap:
                comp_heap[root] = []
            heappush(comp_heap[root], i)
        online = [True] * (c + 1)
        ans = []
        for t, x in queries:
            if t == 1:
                if online[x]:
                    ans.append(x)
                else:
                    root = find(x)
                    heap = comp_heap[root]
                    while heap and not online[heap[0]]:
                        heappop(heap)
                    if heap:
                        ans.append(heap[0])
                    else:
                        ans.append(-1)
            else:  
                online[x] = False
        return ans
